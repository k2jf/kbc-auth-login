<?xml version="1.0" encoding="UTF-8"?>
<model basePath="com/my" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.k2data.com.cn/schema/amf/v1">

    <!-- 所有替换规则依次执行 -->
    <!-- 区分大小写 -->
    <substitutions>
        <substitution includeFileName="false">
            <from>com.k2data.kbc</from>
            <to>com.k2data.kbct</to>
        </substitution>
        <substitution includeFileName="true">
            <from>com/k2data/kbc</from>
            <to>com/k2data/kbct</to>
        </substitution>
        <substitution includeFileName="true">
            <from>announce</from>
            <to>device</to>
        </substitution>
        <substitution includeFileName="true">
            <from>Announce</from>
            <to>Device</to>
        </substitution>
    </substitutions>

    <device name="turbine">
        <attributes>
            <!--<attribute name="code" type="string" title="整机编码"/>-->
            <!--<attribute name="vid" type="integer" title="ID"/>--><!-- 故意使用特别的id名称，并且不放在第一位，以方便暴露ref解析问题 -->
            <attribute name="power" type="string" title="额定功率"/>
            <attribute name="bladeManufacturer" type="string" title="叶片厂商"/>
            <attribute name="towerHeight" type="string" title="塔架高度"/>

            <!--<ref name="saleNet" title="销售网络" target="saleNet" />-->
            <!--<ref name="archiveUnit" title="建档单位" target="archiveUnit" multiple="false" containment="false"/>-->
        </attributes>
    </device>
</model>